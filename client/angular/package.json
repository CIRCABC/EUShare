{
  "name": "client",
  "version": "1.0.1",
  "license": "EUPL-1.1",
  "engines": {
    "node": "^14.15.0 || >=16.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "postinstall": "node ./src/version.js",
    "ng": "ng",
    "start": "ng serve -o --port=8080 -c=dev",
    "start-local": "ng serve -o --port=8080 -c=dev-local ",
    "start-local-docker": "ng serve  -c dev --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "maildev": "maildev",
    "test": "ng test",
    "lint": "ng lint client --format visualstudio",
    "lint-fix": "ng lint client --fix --force --format visualstudio",
    "prettier": "npx gulp prettier",
    "prettier-css": "gulp format:css ",
    "e2e": "ng e2e",
    "licchkchk": "license-check-and-add check -f",
    "licchkadd": "license-check-and-add add",
    "tsc": "tsc --project tsconfig.json --pretty",
    "ncu": "npx npm-check-updates",
    "build-stat": "ng build -c=production --stats-json",
    "bundle-report": "npx webpack-bundle-analyzer dist/stats.json",
    "bundle-report-es2015": "npx webpack-bundle-analyzer dist/stats-es2015.json",
    "doc-build-serve": "npx compodoc -p tsconfig.json -s -r 9090 -o",
    "gen-version": "openapi-generator-cli version",
    "gen-version-list": "openapi-generator-cli version-manager list",
    "gen-eu-share-client": "rimraf src/app/openapi && openapi-generator-cli generate -i ./../../api.yaml -g typescript-angular -o src/app/openapi --additional-properties ngVersion=9.0.0,providedInRoot=true && npm run prettier && npm run tsc && npm run licchkadd && npm run lint-fix",
    "build-sm": "ng build -c=development",
    "source-map-explorer": "source-map-explorer dist/**/*.js",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "spellcheck": "cspell /src/**/*.ts",
    "circleci": "npm run build && npm run licchkchk && npm run lint ",
    "cy:open": "cypress open -b firefox:dev",
    "cy:run": "cypress run -b firefox:dev"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "0.1402.5",
    "@angular-devkit/core": "14.2.5",
    "@angular-devkit/schematics": "14.2.5",
    "@angular/animations": "14.2.5",
    "@angular/cdk": "14.2.4",
    "@angular/common": "14.2.5",
    "@angular/compiler": "14.2.5",
    "@angular/core": "14.2.5",
    "@angular/forms": "14.2.5",
    "@angular/platform-browser": "14.2.5",
    "@angular/platform-browser-dynamic": "14.2.5",
    "@angular/router": "14.2.5",
    "@fortawesome/angular-fontawesome": "0.11.1",
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@ngneat/transloco": "4.1.1",
    "@schematics/angular": "14.2.5",
    "@schematics/update": "0.1102.18",
    "@types/jsrsasign": "10.5.4",
    "angular-oauth2-oidc": "13.0.1",
    "bulma": "0.9.4",
    "bulma-extensions": "6.2.7",
    "core-js": "3.25.5",
    "hammerjs": "2.0.8",
    "jsrsasign": "10.5.27",
    "license-check-and-add": "4.0.5",
    "rxjs": "7.5.7",
    "ubuntu-fontface": "0.1.13",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.5",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.5",
    "@angular/compiler-cli": "14.2.5",
    "@angular/language-service": "14.2.5",
    "@cypress/schematic": "2.1.1",
    "@ngneat/transloco-keys-manager": "3.4.2",
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@types/file-saver": "2.0.5",
    "@types/jasmine": "4.3.0",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.8.3",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "axe-core": "4.4.3",
    "cspell": "6.12.0",
    "cypress": "10.9.0",
    "cypress-axe": "1.0.0",
    "cypress-file-upload": "5.0.8",
    "eslint": "8.24.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "git-describe": "4.1.1",
    "gulp": "4.0.2",
    "gulp-prettier": "4.0.0",
    "jasmine-core": "4.4.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "source-map-explorer": "2.5.3",
    "typescript": "4.8.4"
  }
}
