{
  "name": "client",
  "version": "0.0.0",
  "license": "EUPL-1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build -c=dev",
    "builddocker": "ng build -c=docker",
    "buildprod": "ng build --prod -c=production",
    "test": "ng test",
    "lint": "ng lint client",
    "lint-fix": "ng lint client --fix --force --format msbuild",
    "lint-check-for-prettier-conflicts": "tslint-config-prettier-check ./tslint.json",
    "prettier": "npx gulp prettier",
    "prettier-css": "gulp format:css ",
    "e2e": "ng e2e",
    "licchkadd": "license-check-and-add",
    "tsc": "tsc --project tsconfig.json --pretty",
    "ncu": "npx npm-check-updates",
    "ncu-u": "npx npm-check-updates -u",
    "build-stat": "ng build --prod --stats-json",
    "bundle-report": "npx webpack-bundle-analyzer dist/stats-es5.json",
    "bundle-report-es2015": "npx webpack-bundle-analyzer dist/stats-es2015.json",
    "doc-build-serve": "npx compodoc -p tsconfig.json -s -r 9090 -o"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE_HEADER.txt",
    "exact_paths_method": "INCLUDE",
    "exact_paths": [
      "src/app"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".html",
      ".txt",
      ".json",
      ".sh",
      ".md",
      ".css"
    ],
    "insert_license": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.3",
    "@angular/common": "8.2.3",
    "@angular/compiler": "8.2.3",
    "@angular/core": "8.2.3",
    "@angular/forms": "8.2.3",
    "@angular/platform-browser": "8.2.3",
    "@angular/platform-browser-dynamic": "8.2.3",
    "@angular/router": "8.2.3",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "bulma": "0.7.5",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.9",
    "file-saver": "2.0.2",
    "license-check-and-add": "2.3.6",
    "rxjs": "6.5.2",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.0",
    "@angular/cli": "8.3.0",
    "@angular/compiler-cli": "8.2.3",
    "@angular/language-service": "8.2.3",
    "@types/file-saver": "2.0.1",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.2",
    "codelyzer": "5.1.0",
    "gulp": "4.0.2",
    "gulp-prettier": "2.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "primeng": "8.0.2",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3"
  }
}
