{
  "name": "client",
  "version": "0.0.0",
  "license": "EUPL-1.1",
  "scripts": {
    "postinstall": "node ./src/version.js",
    "ng": "ng",
    "start": "ng serve -o --port=8080",
    "build": "ng build -c=dev",
    "builddevdocker": "ng build -c=devdocker",
    "buildproddocker": "ng build -c=proddocker",
    "buildproddockerdb": "ng build -c=proddockerdb",
    "maildev": "maildev",
    "test": "ng test",
    "lint": "ng lint client",
    "lint-fix": "ng lint client --fix --force --format msbuild",
    "lint-check-for-prettier-conflicts": "tslint-config-prettier-check ./tslint.json",
    "prettier": "npx gulp prettier",
    "prettier-css": "gulp format:css ",
    "e2e": "ng e2e",
    "licchkadd": "license-check-and-add",
    "tsc": "tsc --project tsconfig.json --pretty",
    "ncu": "npx npm-check-updates",
    "build-stat": "ng build --prod --stats-json",
    "bundle-report": "npx webpack-bundle-analyzer dist/stats.json",
    "bundle-report-es2015": "npx webpack-bundle-analyzer dist/stats-es2015.json",
    "doc-build-serve": "npx compodoc -p tsconfig.json -s -r 9090 -o",
    "gen-version": "openapi-generator-cli version",
    "gen-version-list": "openapi-generator-cli version-manager list",
    "gen-eu-share-client": "rimraf src/app/openapi && openapi-generator-cli generate -i ./../../api.yaml -g typescript-angular -o src/app/openapi --additional-properties ngVersion=9.0.0,providedInRoot=true && npm run prettier",
    "gen-eu-share-server": "rimraf ./../../server/src/main/java/com/circabc/easyshare/api && openapi-generator-cli generate -i ./../../api.yaml -g spring -o ./../../server/src/main/java/com/circabc/easyshare/api",
    "build-sm": "ng build --prod",
    "source-map-explorer": "source-map-explorer dist/**/*.js"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE_HEADER.txt",
    "exact_paths_method": "INCLUDE",
    "exact_paths": [
      "src/app"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".html",
      ".txt",
      ".json",
      ".sh",
      ".md",
      ".css"
    ],
    "insert_license": false
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "0.1202.1",
    "@angular-devkit/core": "12.2.1",
    "@angular-devkit/schematics": "12.2.1",
    "@angular/animations": "12.2.1",
    "@angular/cdk": "12.2.1",
    "@angular/common": "12.2.1",
    "@angular/compiler": "12.2.1",
    "@angular/core": "12.2.1",
    "@angular/forms": "12.2.1",
    "@angular/material": "12.2.1",
    "@angular/platform-browser": "12.2.1",
    "@angular/platform-browser-dynamic": "12.2.1",
    "@angular/router": "12.2.1",
    "@fortawesome/angular-fontawesome": "0.9.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@ngneat/transloco": "2.22.0",
    "@schematics/angular": "12.2.1",
    "@schematics/update": "0.1102.12",
    "@types/jsrsasign": "8.0.13",
    "angular-oauth2-oidc": "12.0.2",
    "bufferutil": "4.0.3",
    "bulma": "0.9.3",
    "bulma-extensions": "6.2.7",
    "core-js": "3.16.1",
    "file-saver": "2.0.5",
    "hammerjs": "^2.0.8",
    "jsrsasign": "10.3.0",
    "license-check-and-add": "4.0.2",
    "rxjs": "6.6.7",
    "ubuntu-fontface": "0.1.13",
    "utf-8-validate": "5.0.5",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.1",
    "@angular/cli": "12.2.1",
    "@angular/compiler-cli": "12.2.1",
    "@angular/language-service": "12.2.1",
    "@openapitools/openapi-generator-cli": "2.3.10",
    "@types/file-saver": "2.0.3",
    "@types/jasmine": "3.8.2",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.6.1",
    "codelyzer": "6.0.2",
    "git-describe": "4.0.4",
    "gulp": "4.0.2",
    "gulp-prettier": "4.0.0",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "protractor": "7.0.0",
    "source-map-explorer": "2.5.2",
    "ts-node": "10.2.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.3.5"
  }
}
