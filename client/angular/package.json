{
  "name": "client",
  "version": "1.1.0",
  "license": "EUPL-1.1",
  "engines": {
    "node": "^14.20.0 || ^16.13.0 || >=20.9.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "postinstall": "node ./src/version.js",
    "ng": "ng",
    "start": "cross-env NG_BUILD_LEGACY_SASS=1  ng serve -o --port=8080 -c=dev",
    "start-local": "cross-env NG_BUILD_LEGACY_SASS=1 ng serve -o --port=8080 -c=dev-local ",
    "start-local-docker": "cross-env NG_BUILD_LEGACY_SASS=1 ng serve  -c=local --port 80 --host 0.0.0.0 --disable-host-check",
    "build": "cross-env NG_BUILD_LEGACY_SASS=1 ng build",
    "build-local": "cross-env NG_BUILD_LEGACY_SASS=1 ng build -c local",
    "maildev": "maildev",
    "lint": "ng lint client --format visualstudio",
    "lint-fix": "ng lint client --fix --force --format visualstudio",
    "lint-biome": "biome check ./src",
    "lint-fix-biome": "biome check ./src --apply-unsafe",
    "format-check": "prettier --single-quote --list-different --html-whitespace-sensitivity strict \"src/**/*.html\" \"src/**/*.ts\" \"src/**/*.scss\" ",
    "format-fix": "prettier --write --single-quote --list-different --html-whitespace-sensitivity strict \"src/**/*.html\" \"src/**/*.ts\" \"src/**/*.scss\" ",
    "format-check-scss": "prettier --single-quote --list-different  \"src/**/*.scss\" ",
    "stylelint": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.scss\" --custom-syntax postcss-scss ",
    "stylelint-html": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.html\" ",
    "stylelint-fix": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.scss\" --custom-syntax postcss-scss --fix",
    "stylelint-html-fix": "node ./node_modules/stylelint/bin/stylelint.mjs \"src/**/*.html\" --fix ",
    "e2e": "ng e2e",
    "licchkchk": "license-check-and-add check -f",
    "licchkadd": "license-check-and-add add",
    "tsc": "tsc --project tsconfig.json --pretty",
    "ncu": "npx npm-check-updates",
    "build-stat": "ng build -c=production --stats-json",
    "bundle-report": "npx webpack-bundle-analyzer dist/stats.json",
    "bundle-report-es2015": "npx webpack-bundle-analyzer dist/stats-es2015.json",
    "doc-build-serve": "npx compodoc -p tsconfig.json -s -r 9090 -o",
    "gen-version": "openapi-generator-cli version",
    "gen-version-list": "openapi-generator-cli version-manager list",
    "gen-eu-share-client": "rimraf src/app/openapi && openapi-generator-cli generate -i ./../../api.yaml -g typescript-angular -o src/app/openapi --additional-properties ngVersion=9.0.0,providedInRoot=true  && npm run tsc && npm run licchkadd && npm run lint-fix",
    "gen-eu-captcha-api": "rimraf src/app/openapi/eu-captcha && openapi-generator-cli generate -i ./../../eu-captcha.yaml -g typescript-angular -o src/app/openapi/eu-captcha --additional-properties ngVersion=9.0.0,providedInRoot=true ",
    "build-sm": "ng build -c=development",
    "source-map-explorer": "source-map-explorer dist/**/*.js",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "spellcheck": "cspell /src/**/*.ts",
    "circleci": "npm run format-check && npm run build && npm run licchkchk && npm run lint-biome && npm run lint",
    "cy:open": "cypress open -b firefox:dev",
    "cy:run": "cypress run -b firefox:dev",
    "update-15": "ng update @angular/core@15 @angular/cli@15 --allow-dirty --force"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.1",
    "@angular/common": "17.3.1",
    "@angular/compiler": "17.3.1",
    "@angular/core": "17.3.1",
    "@angular/forms": "17.3.1",
    "@angular/material": "17.3.1",
    "@angular/platform-browser": "17.3.1",
    "@angular/platform-browser-dynamic": "17.3.1",
    "@angular/router": "17.3.1",
    "@fortawesome/angular-fontawesome": "0.14.1",
    "@fortawesome/fontawesome-free": "6.5.1",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@ngneat/transloco": "6.0.4",
    "@schematics/angular": "17.3.1",
    "@types/jsrsasign": "10.5.13",
    "angular-oauth2-oidc": "17.0.1",
    "angularx-qrcode": "17.0.0",
    "bulma": "0.9.4",
    "bulma-extensions": "6.2.7",
    "chart.js": "4.4.2",
    "core-js": "3.36.1",
    "hammerjs": "2.0.8",
    "jsrsasign": "11.1.0",
    "license-check-and-add": "4.0.5",
    "material-icons": "^1.13.12",
    "ng2-charts": "6.0.0",
    "prettier": "3.2.5",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "ubuntu-fontface": "0.1.13",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.1",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.1",
    "@angular/compiler-cli": "17.3.1",
    "@biomejs/biome": "1.6.1",
    "@cypress/schematic": "2.5.1",
    "@ngneat/transloco-keys-manager": "3.8.0",
    "@openapitools/openapi-generator-cli": "2.13.1",
    "@types/file-saver": "2.0.7",
    "@types/jasmine": "5.1.4",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "axe-core": "4.8.4",
    "cross-env": "7.0.3",
    "cspell": "8.6.0",
    "cypress": "13.7.1",
    "cypress-axe": "1.5.0",
    "cypress-file-upload": "5.0.8",
    "eslint": "8.57.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "git-describe": "4.1.1",
    "jasmine-core": "5.1.2",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "source-map-explorer": "2.5.3",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "typescript": "5.4.3"
  }
}
