{
  "name": "client",
  "version": "1.0.1",
  "license": "EUPL-1.1",
  "engines": {
    "node": "^14.20.0 || ^16.13.0 || >=18.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "postinstall": "node ./src/version.js",
    "ng": "ng",
    "start": "cross-env NG_BUILD_LEGACY_SASS=1  ng serve -o --port=8080 -c=dev",
    "start-local": "cross-env NG_BUILD_LEGACY_SASS=1 ng serve -o --port=8080 -c=dev-local ",
    "start-local-docker": "cross-env NG_BUILD_LEGACY_SASS=1 ng serve  -c dev --host 0.0.0.0 --disable-host-check",
    "build": "cross-env NG_BUILD_LEGACY_SASS=1 ng build",
    "maildev": "maildev",
    "test": "ng test",
    "lint": "ng lint client --format visualstudio",
    "lint-fix": "ng lint client --fix --force --format visualstudio",
    "lint-rome": "rome check ./src",
    "lint-fix-rome": "rome check ./src --apply-suggested",
    "prettier": "npx gulp prettier",
    "prettier-css": "gulp format:css ",
    "e2e": "ng e2e",
    "licchkchk": "license-check-and-add check -f",
    "licchkadd": "license-check-and-add add",
    "tsc": "tsc --project tsconfig.json --pretty",
    "ncu": "npx npm-check-updates",
    "build-stat": "ng build -c=production --stats-json",
    "bundle-report": "npx webpack-bundle-analyzer dist/stats.json",
    "bundle-report-es2015": "npx webpack-bundle-analyzer dist/stats-es2015.json",
    "doc-build-serve": "npx compodoc -p tsconfig.json -s -r 9090 -o",
    "gen-version": "openapi-generator-cli version",
    "gen-version-list": "openapi-generator-cli version-manager list",
    "gen-eu-share-client": "rimraf src/app/openapi && openapi-generator-cli generate -i ./../../api.yaml -g typescript-angular -o src/app/openapi --additional-properties ngVersion=9.0.0,providedInRoot=true && npm run prettier && npm run tsc && npm run licchkadd && npm run lint-fix",
    "build-sm": "ng build -c=development",
    "source-map-explorer": "source-map-explorer dist/**/*.js",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "spellcheck": "cspell /src/**/*.ts",
    "circleci": "npm run build && npm run licchkchk && npm run lint-rome ",
    "cy:open": "cypress open -b firefox:dev",
    "cy:run": "cypress run -b firefox:dev",
    "update-15": "ng update @angular/core@15 @angular/cli@15 --allow-dirty --force"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.0",
    "@angular/common": "15.1.0",
    "@angular/compiler": "15.1.0",
    "@angular/core": "15.1.0",
    "@angular/forms": "15.1.0",
    "@angular/platform-browser": "15.1.0",
    "@angular/platform-browser-dynamic": "15.1.0",
    "@angular/router": "15.1.0",
    "@fortawesome/angular-fontawesome": "0.12.1",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@ngneat/transloco": "4.2.2",
    "@schematics/angular": "15.1.1",
    "@schematics/update": "0.1102.18",
    "@types/jsrsasign": "10.5.4",
    "angular-oauth2-oidc": "15.0.1",
    "bulma": "0.9.4",
    "bulma-extensions": "6.2.7",
    "chart.js": "^4.1.2",
    "core-js": "3.27.1",
    "hammerjs": "2.0.8",
    "jsrsasign": "10.6.1",
    "license-check-and-add": "4.0.5",
    "ng2-charts": "^4.1.1",
    "rxjs": "7.8.0",
    "tslib": "2.4.1",
    "ubuntu-fontface": "0.1.13",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.1",
    "@angular/cli": "15.1.1",
    "@angular/compiler-cli": "15.1.0",
    "@cypress/schematic": "2.4.0",
    "@ngneat/transloco-keys-manager": "3.5.0",
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@types/file-saver": "2.0.5",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "axe-core": "4.6.2",
    "cross-env": "7.0.3",
    "cspell": "6.18.1",
    "cypress": "12.3.0",
    "cypress-axe": "1.2.0",
    "cypress-file-upload": "5.0.8",
    "eslint": "8.32.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "git-describe": "4.1.1",
    "gulp": "4.0.2",
    "gulp-prettier": "4.0.0",
    "jasmine-core": "4.5.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "rome": "11.0.0",
    "source-map-explorer": "2.5.3",
    "typescript": "4.8.4"
  }
}
